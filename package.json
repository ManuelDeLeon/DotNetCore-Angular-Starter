{
  "name": "Angular2Spa",
  "version": "0.0.0",
  "scripts": {
    "test": "karma start",
    "test-single": "karma start --single-run --env.coverage",
    "coverage": "rimraf ClientCoverage && npm run test-single && http-server -c-1 -o -p 9875 ClientCoverage",
    "lint-ts": "tslint ClientApp/**/*.ts",
    "lint-html": "htmlhint ClientApp/**/*.html -c htmlhint.json",
    "lint": "npm run lint-ts && npm run lint-html",
    "build-dev": "webpack --progress --colors --config webpack.config.vendor.js && webpack --progress --colors",
    "build-prod": "webpack -p --progress --colors --env.prod --config webpack.config.vendor.js && webpack -p --progress --colors --env.prod",
	  "profile-main": "webpack --progress --colors --profile",
    "stats-main": "webpack --progress --colors --json | .\\node_modules\\.bin\\webpack-bundle-size-analyzer",
	  "stats-vendor": "webpack --progress --colors --config webpack.config.vendor.js --json | .\\node_modules\\.bin\\webpack-bundle-size-analyzer",
    "start": "npm run dev",
    "dev": "cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "prod": "cross-env ASPNETCORE_ENVIRONMENT=Production dotnet run",
    "initialize": "npm install && npm run build-dev && dotnet restore && del ClientApp\\*.js* /q/s"
  },
  "dependencies": {
    "@angular/common": "2.0.2",
    "@angular/compiler": "2.0.2",
    "@angular/core": "2.0.2",
    "@angular/forms": "2.0.2",
    "@angular/http": "2.0.2",
    "@angular/platform-browser": "2.0.2",
    "@angular/platform-browser-dynamic": "2.0.2",
    "@angular/platform-server": "2.0.2",
    "@angular/router": "3.0.2",
    "@types/node": "^6.0.42",
    "angular2-platform-node": "~2.0.11",
    "angular2-template-loader": "^0.6.0",
    "angular2-universal": "~2.0.11",
    "angular2-universal-polyfills": "~2.0.11",
    "aspnet-prerendering": "^2.0.0",
    "aspnet-webpack": "^1.0.17",
    "bootstrap": "^3.3.7",
    "codelyzer": "^2.0.0-beta.3",
    "cross-env": "^3.1.3",
    "css": "^2.2.1",
    "css-loader": "0.14.5",
    "es6-shim": "^0.35.1",
    "event-source-polyfill": "^0.0.7",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "0.0.4",
    "html-loader": "^0.4.4",
    "htmlhint": "^0.9.13",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.1",
    "json-loader": "^0.5.4",    
    "lodash": "4.17.4",
    "less": "^2.7.1",
    "node-noop": "^1.0.0",
    "node-sass": "^3.13.0",
    "preboot": "^4.5.2",
    "raw-loader": "^0.5.1",
    "rxjs": "5.0.0-beta.12",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^0.8.2",
    "tslint": "^4.0.2",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/cheerio": "0.17.31",
    "@types/jasmine": "^2.5.38",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "karma-webpack": "^1.8.0",
    "sourcemap-istanbul-instrumenter-loader": "^0.2.0",
    "text-loader": "0.0.1",
    "webpack-bundle-size-analyzer": "^2.2.0"
  }
}
