{
  "name": "Mcangular",
  "version": "0.0.0",
  "//": [
    "What scripts do:",
    "test: Run tests and watch files for changes.",
    "test-single: Run tests once.",
    "coverate: Show a test code coverage report.",
    "lint: Lint both TypeScript and HTML files. Check if the files follow our conventions.",
    "lint-ts: Lint TypeScript files. Check if the files follow our conventions.",
    "lint-html: Lint HTML files. Check if the files follow our conventions.",
    "build-dev: Compile and bundle the TypeScript code for development. This will also create the vendor bundle with our external references.",
    "build-prod: Compile and bundle the TypeScript code for production. This will also create the vendor bundle with our external references. See the size script.",
    "size: Show the size of each file and external dependency.",
    "start: Run the application in development mode. Same as dev script.",
    "dev: Run the application in development mode.",
    "prod: Run the application in production mode.",
    "initialize: Install all dependencies and build the application."
  ],
  "scripts": {
    "test": "karma start",
    "test-single": "karma start --single-run --env.coverage",
    "coverage": "rimraf ClientCoverage && npm run test-single && http-server -c-1 -o -p 9875 ClientCoverage",
    "lint": "npm run lint-ts && npm run lint-html",
    "lint-ts": "tslint ClientApp/**/*.ts",
    "lint-html": "htmlhint ClientApp/**/*.html -c htmlhint.json",
    "build-dev": "webpack --progress --config webpack.config.vendor.js && webpack --progress",
    "build-prod": "webpack -p --progress --env.prod --config webpack.config.vendor.js && webpack -p --progress --env.prod",
    "size": "webpack -p --progress --env.prod --env.size --config webpack.config.vendor.js && webpack -p --progress --env.prod --env.size",
    "start": "npm run dev",
    "dev": "cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "prod": "cross-env ASPNETCORE_ENVIRONMENT=Production dotnet run",
    "initialize": "npm install && npm run build-dev && dotnet restore"
  },
  "dependencies": {
    "@angular/common": "^2.4.5",
    "@angular/compiler": "^2.4.5",
    "@angular/core": "^2.4.5",
    "@angular/forms": "^2.4.5",
    "@angular/http": "^2.4.5",
    "@angular/platform-browser": "^2.4.5",
    "@angular/platform-browser-dynamic": "^2.4.5",
    "@angular/platform-server": "^2.4.5",
    "@angular/router": "^3.0.2",
    "@types/chai": "^3.4.34",
    "@types/cheerio": "0.17.31",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.42",
    "angular2-platform-node": "~2.0.11",
    "angular2-template-loader": "^0.6.0",
    "angular2-universal": "^2.1.0-rc.1",
    "angular2-universal-patch": "^0.2.1",
    "angular2-universal-polyfills": "^2.1.0-rc.1",
    "aspnet-prerendering": "^2.0.0",
    "aspnet-webpack": "^1.0.17",
    "awesome-typescript-loader": "3.0.0-beta.13 || ^3.0.0",
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "codelyzer": "^2.0.0-beta.3",
    "cross-env": "^3.1.3",
    "css": "^2.2.1",
    "css-loader": "^0.26.1",
    "es6-shim": "^0.35.1",
    "event-source-polyfill": "^0.0.7",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "0.0.4",
    "html-loader": "^0.4.4",
    "htmlhint": "^0.9.13",
    "http-server": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "jasmine-core": "^2.5.2",
    "jquery": "^2.2.1",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.4.0",
    "karma-webpack": "^2.0.2",
    "less": "^2.7.1",
    "lodash": "4.17.4",
    "node-sass": "^4.5.0",
    "preboot": "^4.5.2",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.1",
    "rxjs": "^5.0.1",
    "sass-loader": "^4.1.1",
    "sourcemap-istanbul-instrumenter-loader": "^0.2.0",
    "style-loader": "^0.13.1",
    "text-loader": "0.0.1",
    "to-string-loader": "^1.1.5",
    "tslint": "^4.0.2",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.5",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1",
    "zone.js": "^0.7.6"
  }
}
